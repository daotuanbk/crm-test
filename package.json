  "scripts": {
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "setup:testdb": "mongo < ./__test__/scripts/setupdb.js && mongorestore --db=techkids-edu-crm-test ./__test__/testdb/dump/techkids-edu-crm",
    "watch": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps --ignore src/**/*.test.ts",
    "test": "./node_modules/cross-env/dist/bin/cross-env.js NO_CLIENT=true NO_CRON_JOB=true NODE_ENV=test jest --detectOpenHandles --verbose --testTimeout=30010",
    "test:window": "cross-env NO_CLIENT=true NO_CRON_JOB=true NODE_ENV=test jest --detectOpenHandles --verbose --testTimeout=30010",
    "devServerAPI": "./node_modules/cross-env/dist/bin/cross-env.js NO_QUEUE=true NO_CLIENT=true NO_CRON_JOB=true nodemon --watch ./src --exec 'ts-node -r tsconfig-paths/register' ./src/index.ts | ./node_modules/.bin/bunyan",
    "devServerAPI:window": "cross-env NO_CLIENT=true NO_CRON_JOB=true nodemon --watch ./src --exec 'ts-node -r tsconfig-paths/register' ./src/index.ts | ./node_modules/.bin/bunyan",
    "devFrontEnd": "./node_modules/cross-env/dist/bin/cross-env.js NO_CRON_JOB=true nodemon --watch ./src/client --exec \"ts-node -r tsconfig-paths/register\" ./src/index.ts | ./node_modules/.bin/bunyan",
    "devFrontEnd:window": "cross-env NO_CRON_JOB=true nodemon --watch ./src/client --exec \"ts-node -r tsconfig-paths/register\" ./src/index.ts | ./node_modules/.bin/bunyan",
    "dev": "./node_modules/cross-env/dist/bin/cross-env.js NO_CRON_JOB=true nodemon --watch ./src --exec \"ts-node -r tsconfig-paths/register\" ./src/index.ts | ./node_modules/.bin/bunyan",
    "dev:window": "nodemon --watch ./src --exec \"ts-node -r tsconfig-paths/register\" ./src/index.ts | ./node_modules/.bin/bunyan",
    "start": "./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production node ./dist/index.js -p $PORT | ./node_modules/.bin/bunyan",
    "start:window": "SET NODE_ENV=production && node ./dist/index.js -p $PORT | ./node_modules/.bin/bunyan",
    "build": "shx rm -rf ./dist && yarn build:less && next build ./src/client && yarn watch && yarn copyToDist",
    "build:less": "shx mkdir ./dist && shx mkdir ./dist/client && shx cp -R ./src/client/custom-antd.less ./dist/client/",
    "lint": "./node_modules/.bin/tslint -c ./tslint.json -p ./tsconfig.json",
    "lint:fix": "./node_modules/.bin/tslint --fix -c ./tslint.json -p ./tsconfig.json",
    "copyToDist": "node ./dist/server/scripts/copyToDist",
    "generateProxy": "./src/client/nswag/nswag run",
    "createIndexes": "node ./dist/server/scripts/createIndexes",
    "createSystemConfigs": "node ./dist/server/scripts/createSystemConfigs",
    "createSampleCourses": "node ./dist/server/scripts/createSampleCourses",
    "seedDb": "node ./dist/server/scripts/seedDb",
    "updatePermissions": "node ./dist/server/scripts/updatePermissions",
    "updateRoles": "node ./dist/server/scripts/updateRoles",
    "updateFbAccessToken": "node ./dist/server/scripts/updateFbAccessToken",
    "updateFbCampaignAccessToken": "node ./dist/server/scripts/updateFbCampaignAccessToken",
    "createProspectingLists": "node ./dist/server/scripts/createProspectingLists",
    "createInitialCombo": "node ./dist/server/scripts/createInitialCombos",
    "convertAutomailConfig": "node ./dist/server/scripts/convertAutomailConfig",
    "updatePaymentForCourses": "node ./dist/server/scripts/updatePaymentForCourses",
    "updateMapping": "node ./dist/server/scripts/updateMapping",
    "updateCourseTuitionForLeads": "node ./dist/server/scripts/updateCourseTuitionForLeads",
    "updateStudentId": "node ./dist/server/scripts/updateStudentId",
    "updateContactRelations": "node ./dist/server/scripts/updateContactRelationsForLead",
    "updateSourceForContacts": "node ./dist/server/scripts/updateSourceForContact",
    "updateLeadTuitionPercent": "node ./dist/server/scripts/updateLeadTuitionPercent",
    "updateV2Contact": "node ./dist/server/scripts/updateV2Contact",
    "build-dev-fe": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "watch-dev-be": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --watch --copy-files",
    "dev-fe": "yarn build-dev-fe && NO_QUEUE=true NO_CRON_JOB=true node ./dist/index.js | ./node_modules/.bin/bunyan",
    "dev-be": "yarn watch-dev-be && NO_QUEUE=true NO_CLIENT=true NO_CRON_JOB=true nodemon --watch ./dist/server --watch ./dist/common --nolazy ./dist/index.js | ./node_modules/.bin/bunyan",
    "dev-all-be": "yarn watch-dev-be & NO_CRON_JOB=true NO_QUEUE=true nodemon --watch ./dist/server --watch ./dist/common --nolazy ./dist/server-standalone.js",
    "dev-all-fe": "yarn build-dev-fe & NO_CRON_JOB=true NO_QUEUE=true node ./dist/client-standalone.js",
    "dev-all-be:withQueue": "yarn watch-dev-be & NO_CRON_JOB=true nodemon --watch ./dist/server --watch ./dist/common --nolazy ./dist/server-standalone.js",
    "build-server": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "watch-new": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --watch"
  },
  "types": "dist/index.d.ts",
  "version": "0.1.0"
